!!!
html
  head
    meta(name="viewport", content="width=device-width, minimum-scale=1.0, maximum-scale=1.0")
    include includes/config
    title= settings.BlogName
    link(rel='stylesheet', href='/bootstrap/css/bootstrap.css')
    link(rel='stylesheet', href='/stylesheets/style.css')
  body
    include includes/head
    .container
      .row
        .span8 
          != body
        .span4#twitter_div
          a(href="http://twitter.com/deevus", id="twitter-link", target="_blank")
            \ Latest Tweets:
          span#twitter_update_list
  script(src='/bootstrap/js/bootstrap.js', type='text/javascript')
  script(src='http://twitter.com/javascripts/blogger.js', type='text/javascript')
  script(src='https://api.twitter.com/1/statuses/user_timeline.json?screen_name=deevus&include_rts=true&count=5&callback=twitterCallback2', type='text/javascript')
  
